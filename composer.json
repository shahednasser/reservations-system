{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "dompdf/dompdf": "^2.0.1",
        "fideloper/proxy": "^4.0",
        "laravel-notification-channels/webpush": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "mpdf/mpdf": "^7.1",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "krlove/eloquent-model-generator": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
          "app/Common.php",
          "app/CompareFunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "cp public/fonts/Amiri-Bold.ttf vendor/mpdf/mpdf/ttfonts/",
            "cp public/fonts/Amiri-BoldItalic.ttf vendor/mpdf/mpdf/ttfonts/",
            "cp public/fonts/Amiri-Italic.ttf vendor/mpdf/mpdf/ttfonts/",
            "cp public/fonts/Amiri-Regular.ttf vendor/mpdf/mpdf/ttfonts/"
        ],
        "post-update-cmd": [
            "cp public/fonts/Amiri-Bold.ttf vendor/mpdf/mpdf/ttfonts/",
            "cp public/fonts/Amiri-BoldItalic.ttf vendor/mpdf/mpdf/ttfonts/",
            "cp public/fonts/Amiri-Italic.ttf vendor/mpdf/mpdf/ttfonts/",
            "cp public/fonts/Amiri-Regular.ttf vendor/mpdf/mpdf/ttfonts/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
